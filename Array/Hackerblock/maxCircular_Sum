#include<bits/stdc++.h>
using namespace std;

int maxSubarray_Sum(int arr[], int n)
{
     int b_sum=INT_MIN,sum=0;
        for(int i=0;i<n;i++)
        {
            sum+=arr[i];
            if(b_sum<sum)
            {
                b_sum=sum;

            }
            if(sum<0)
            {
                sum=0;

            }

        }
        return b_sum;

}


int cumulativeSum(int arr[], int n)
{
    int sum=0;
    for(int i=0;i<n;i++)
    sum+=arr[i];
    return sum;

}
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        int arr[n];
        int arr2[n];
        for(int i=0;i<n;i++)
        {
            cin>>arr[i];
            arr2[i]=-arr[i];

        }
        int candidate1=cumulativeSum(arr, n);
        int candidate2=maxSubarray_Sum(arr2, n);
        cout<<cadidate1+cadidate2<<"\n";



    }

}
